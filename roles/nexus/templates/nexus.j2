[Unit]
    Description=nexus.service
    After=network.target

[Service]
    Type=forking
    LimitNOFILE=65536
    User=nexus
    Group=nexus
    ExecStart=/opt/nexus/nexus-3.58.1-02/bin/nexus start
    ExecStop=/opt/nexus/nexus-3.58.1-02/bin/nexus stop
    User=nexus
    Restart=on-abort
 [Install]
    WantedBy=multi-user.target
